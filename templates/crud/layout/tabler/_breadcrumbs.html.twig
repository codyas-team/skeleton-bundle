{% set breadcrumbs = csk_menu_breadcrumb(tabler_menu(app.request)) %}
<div class="page-pretitle">
    <ol class="breadcrumb" aria-label="breadcrumbs">
        <li class="breadcrumb-item"><a
                href="{{ path(csk_option('templating.home_path')) }}">{{ csk_option('templating.home_label') | trans() }}</a>
        </li>
        {% for breadcrumb in breadcrumbs %}
            {% if breadcrumb.route != 'item_symfony_route' %}
                <li class="breadcrumb-item">
                    <a href="{{ path(breadcrumb.route) }}">{{ breadcrumb.label }}</a>
                </li>
            {% endif %}

        {% endfor %}
    </ol>
</div>

